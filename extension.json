{
	"name": "Zest",
	"version": "0.1",
	"author": [
		"Jayanth Vikash Saminathan",
		"Naresh Kumar Babu",
		"Sanjay Thiyagarajan",
        "Yaron Koren"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Zest",
	"descriptionmsg": "zest-desc",
	"license-name": "GPL-2.0-or-later",
	"requires": {
		"MediaWiki": ">= 1.41"
	},
	"type": "specialpage",
	"GroupPermissions": {
		"sysop": {
			"zest": true
		}
	},
	"AvailableRights": [
		"zest"
	],
	"config": {
		"WTDistributionListURL": "https://www.mediawiki.org/wiki/Recommended_revisions/1.43"
	},
	"SpecialPages": {
		"Zest": {
			"class": "MediaWiki\\Extension\\Zest\\SpecialZest"
		}
	},
	"MessagesDirs": {
		"Zest": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.Zest": {
			"class": "MediaWiki\\ResourceLoader\\CodexModule",
			"dependencies": [
				"vue"
			],
			"packageFiles": [
				"init.js",
				"components/App.vue",
				"components/Extensions.vue",
				"components/Skins.vue"
			],
			"codexComponents": [
				"CdxButton",
				"CdxTextInput",
				"CdxTab",
				"CdxTabs",
				"CdxTable"
			],
			"messages": [
				"zest-extensions-tab-name",
				"zest-skins-tab-name",
				"zest-api-error-loadedelsewhere",
				"zest-api-error-alreadyinstalled",
				"zest-api-error-notinstalled",
				"zest-api-error-invalidtype",
				"zest-api-error-composerfile"
			]
		},
		"ext.ZestStyles": {
			"styles": [
				"ext.ZestStyles.less"
			]
		}
	},
	"APIModules": {
		"zest": {
			"class": "MediaWiki\\Extension\\Zest\\ZestAPI"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "Zest/resources"
	},
	"ExtensionMessagesFiles": {
		"ZestAlias": "Zest.i18n.alias.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\Zest\\": "includes"
	},
	"manifest_version": 1
}
