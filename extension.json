{
	"name": "Wikitweak",
	"version": "0.1",
	"author": [
		"Jayanth Vikash Saminathan",
		"Naresh Kumar Babu",
		"Sanjay Thiyagarajan",
        "Yaron Koren"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Wikitweak",
	"descriptionmsg": "wikitweak-desc",
	"license-name": "GPL-2.0-or-later",
	"requires": {
		"MediaWiki": ">= 1.41"
	},
	"type": "specialpage",
	"GroupPermissions": {
		"sysop": {
			"wikitweak": true
		}
	},
	"AvailableRights": [
		"wikitweak"
	],
	"config": {
		"WTDistributionListURL": "https://www.mediawiki.org/wiki/Recommended_revisions/1.43"
	},
	"SpecialPages": {
		"Wikitweak": {
			"class": "MediaWiki\\Extension\\Wikitweak\\SpecialWikitweak"
		}
	},
	"MessagesDirs": {
		"Wikitweak": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.Wikitweak": {
			"class": "MediaWiki\\ResourceLoader\\CodexModule",
			"dependencies": [
				"vue"
			],
			"packageFiles": [
				"init.js",
				"components/App.vue",
				"components/Extensions.vue",
				"components/Skins.vue"
			],
			"codexComponents": [
				"CdxButton",
				"CdxTextInput",
				"CdxTab",
				"CdxTabs",
				"CdxTable"
			],
			"messages": [
				"wikitweak-extensions-tab-name",
				"wikitweak-skins-tab-name"
			]
		},
		"ext.WikitweakStyles": {
			"styles": [
				"ext.WikitweakStyles.less"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "Wikitweak/resources"
	},
	"ExtensionMessagesFiles": {
		"WikitweakAlias": "Wikitweak.i18n.alias.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\Wikitweak\\": "includes"
	},
	"manifest_version": 1
}
